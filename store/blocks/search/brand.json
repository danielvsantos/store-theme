{
  "store.search#brand": {
    "blocks": [
      "search-result-layout#brand"
    ],
    "props": {
      "context": {
        "orderByField": "OrderByReleaseDateDESC",
        "hideUnavailableItems": true,
        "maxItemsPerPage": 10,
        "skusFilter": "FIRST_AVAILABLE"
      }
    }
  },

  "search-result-layout#brand": {
    "blocks": [
      "search-result-layout.desktop.powerplanet#brand",
      "search-result-layout.mobile.powerplanet#brand",
      "search-not-found-layout"
    ]
  },

  "search-result-layout.desktop.powerplanet#brand": {
    "children": [
      "flex-layout.row#searchbread",
      "flex-layout.row#searchtitle",
      "flex-layout.row#brand-banner",
      "flex-layout.row#brand-top-description",
      "flex-layout.row#shelf-brand-top",
      "flex-layout.row#brand-top-category-brands",
      "__fold__",
      "flex-layout.row#shelf-brand-new",
      "flex-layout.row#brand-essencial-brands",
      "flex-layout.row#result",
      "flex-layout.row#brand-description"
    ]
  },

  "search-result-layout.mobile.powerplanet#brand": {
    "children": [
      "search-content"
    ],
    "props": {
      "preserveLayoutOnMobile": true
    }
  },

  "flex-layout.row#brand-banner": {
    "children": [
      "slider-layout#brand-banner"
    ]
  },

  "slider-layout#brand-banner": {
    "children": [
      "info-card#brandBanner1"
    ],
    "props": {
      "autoPlay": {
          "timeout": 5000,
          "stopOnHover": false
      },
      "showNavigationArrows": "mobileOnly",
      "showPaginationDots": "never",
      "infinite": false,
      "itemsPerPage": {
        "desktop": 5, 
          "tablet": 3, 
          "phone": 2
      },
      "blockClass": "brandBanner"
    }
  },

  "info-card#brandBanner1": {
    "props": {
      "imageUrl": "https://images.unsplash.com/photo-1524185962737-ea7c028a12cd?ixlib=rb-1.2.1&auto=format&fit=crop&w=1350&q=80",
      "blockClass": "brandBanner-info-card"
    }
  },

  "flex-layout.row#shelf-brand-top": {
    "children": [
      "flex-layout.col#shelf-brand-top"
    ]
  },
  "flex-layout.col#shelf-brand-top": {
    "children": [
      "rich-text#shelf-brand-top",
      "shelf#shelf-brand-top"
    ]
  },
  "rich-text#shelf-brand-top": {
    "props": {
      "text": "rich-text#shelf-brand-top",
      "blockClass": "test-blocktitle"
    }
  },
  "shelf#shelf-brand-top": {
    "blocks": [
      "product-summary.shelf"
    ],
    "props": {
      "orderBy": "OrderByTopSaleDESC",
      "paginationDotsVisibility": "desktopOnly",
      "skusFilter": "FIRST_AVAILABLE",
      "productList": {
        "maxItems": 10,
        "itemsPerPage": 5,
        "minItemsPerPage": 1,
        "scroll": "BY_PAGE",
        "arrows": true,
        "titleText": "ofertas top"
      }
    }
  },
  "flex-layout.row#brand-description": {
    "children": [
      "flex-layout.col#brand-description"
    ]
  },
  "flex-layout.row#brand-top-description": {
    "children": [
      "flex-layout.col#brand-top-description"
    ]
  },
  "flex-layout.col#brand-description": {
    "children": [
      "rich-text#brand-description-content"
    ]
  },
  "flex-layout.col#brand-top-description": {
    "children": [
      "rich-text#brand-description-top-title",
      "rich-text#brand-description-top-content"
    ]
  },
  "rich-text#brand-description-content": {
    "props": {
      "text": "rich-text#brand-description-content",
      "blockClass": "test-blocktitle"
    }
  },
  "rich-text#brand-description-top-title": {
    "props": {
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "text": "rich-text#brand-description-top-title",
      "blockClass": "brand-top-description-title"
    }
  },
  "rich-text#brand-description-top-content": {
    "props": {
      "text": "rich-text#brand-top-description-content",
      "blockClass": "brand-top-description-content"
    }
  },

  "flex-layout.row#brand-top-category-brands": {
    "children": [
      "flex-layout.col#brand-top-category-brands"
    ]
  },
  "flex-layout.col#brand-top-category-brands": {
    "children": [
      "rich-text#brand-top-category-brands",
      "powerplanet-top-category-brands"
    ]
  },
  "rich-text#brand-top-category-brands": {
    "props": {
      "text": "rich-text#brand-top-category-brands",
      "blockClass": "brandTopCategoryBrands"
    }
  },

  "flex-layout.row#shelf-brand-new": {
    "children": [
      "flex-layout.col#shelf-brand-new"
    ]
  },
  "flex-layout.col#shelf-brand-new": {
    "children": [
      "rich-text#shelf-brand-new",
      "shelf#shelf-brand-new"
    ]
  },
  "rich-text#shelf-brand-new": {
    "props": {
      "text": "rich-text#shelf-brand-new",
      "blockClass": "test-blocktitle"
    }
  },
  "shelf#shelf-brand-new": {
    "blocks": [
      "product-summary.shelf"
    ],
    "props": {
      "orderBy": "OrderByTopSaleDESC",
      "paginationDotsVisibility": "desktopOnly",
      "skusFilter": "FIRST_AVAILABLE",
      "productList": {
        "maxItems": 10,
        "itemsPerPage": 5,
        "minItemsPerPage": 1,
        "scroll": "BY_PAGE",
        "arrows": true,
        "titleText": "novedades"
      }
    }
  },

  "flex-layout.row#brand-essencial-brands": {
    "children": [
      "flex-layout.col#brand-essencial-brands"
    ]
  },
  "flex-layout.col#brand-essencial-brands": {
    "children": [
      "rich-text#brand-essencial-brands",
      "powerplanet-top-category-brands"
    ]
  },
  "rich-text#brand-essencial-brands": {
    "props": {
      "text": "rich-text#brand-essencial-brands",
      "blockClass": "brandEssencialBrands"
    }
  },

  "flex-layout.row#category-description-bottom": {
    "children": [
      "powerplanet-category-description-bottom"
    ]
  }
}